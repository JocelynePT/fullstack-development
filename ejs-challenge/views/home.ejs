<%- include("partials/header"); -%>
    <h1>Home</h1>
    <p><%= startContent %></p>
    <% posts.forEach(function(post) { %>
        <% const length = 100; %>
        <% const content = post.content; %>
        <% const truncatedContent = content.substring(0, length); %>
        <% const title = post.title; %>
        <h1><%= title %> </h1>
        <p><%= truncatedContent %> ...<a href= "/posts/<%= title %>" >Read More</a></p>
    <% }); %>
<%- include("partials/footer"); -%>